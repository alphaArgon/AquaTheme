{
	"name": "Aqua (Light)",

	"semanticHighlighting": true,

	"semanticTokenColors": {
		"macro": {
			"foreground": "#885500",
			"fontStyle": "",
		},

		//  Local Variables:
		// "parameter": "#112244",
		"variable": "#112244",

		//  Project Fields:
		"function": "#0077AA",
		"property": "#0077AA",
		"method": "#0077AA",
		"enumMember": "#0077AA",

		//  Project Types:
		"type": "#0044BB",
		"enum": "#0044BB",
		"class": "#0044BB",
		"struct": "#0044BB",
		"interface": "#0044BB",
		"namespace": "#0044BB",
		"typeParameter": "#0044BB",

		//  Support Fields:
		"variable.builtin": "#9955AA",
		"function.builtin": "#9955AA",
		"property.builtin": "#9955AA",
		"method.builtin": "#9955AA",
		"enumMember.builtin": "#9955AA",
		"typeParameter.builtin": "#9955AA",
		"variable.defaultLibrary": "#9955AA",
		"function.defaultLibrary": "#9955AA",
		"property.defaultLibrary": "#9955AA",
		"method.defaultLibrary": "#9955AA",
		"enumMember.defaultLibrary": "#9955AA",
		"typeParameter.defaultLibrary": "#9955AA",
		"magicFunction": "#9955AA",

		//  Support Types:
		"type.builtin": "#7726BB",
		"enum.builtin": "#7726BB",
		"class.builtin": "#7726BB",
		"struct.builtin": "#7726BB",
		"interface.builtin": "#7726BB",
		"type.defaultLibrary": "#7726BB",
		"enum.defaultLibrary": "#7726BB",
		"class.defaultLibrary": "#7726BB",
		"struct.defaultLibrary": "#7726BB",
		"interface.defaultLibrary": "#7726BB",
		"namespace.defaultLibrary": "#7726BB",

		// "selfParameter": {
		// 	"foreground": "#BB3355",
		// 	"fontStyle": "bold",
		// }
	},

	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
			],
			"settings": {
				"foreground": "#667788",
				"fontStyle": "",
			}
		},
		{
			"name": "Preprocessor",
			"scope": [
				"meta.preprocessor",
				"markup.inline.raw",  // Markdown
				"markup.fenced_code.block",  // Markdown
			],
			"settings": {
				"foreground": "#885500",
				"fontStyle": "",
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"constant.other.color",
				"storage.type.string",
				"string.other.link",  // Markdown
				"constant.other.reference.link",  // Markdown
			],
			"settings": {
				"foreground": "#228800",
				"fontStyle": "",
			}
		},
		{
			"name": "Number",
			"scope": [
				"constant.numeric",
				"string.quoted.single.c",
				"string.quoted.single.cpp",
				"string.quoted.single.objc",
				"string.quoted.single.objcpp",
				"storage.type.numeric",
				"storage.type.number",
				"keyword.other.unit",  // CSS
				"entity.other.keyframe-offset.percentage",  // CSS
			],
			"settings": {
				"foreground": "#CC7700",
				"fontStyle": "",
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword.control",
				"keyword.other",
				"keyword.operator.new",
				"keyword.operator.delete",
				"keyword.operator.sizeof",
				"keyword.operator.alignof",
				"keyword.operator.typeid",
				"keyword.operator.expression",
				"keyword.operator.logical.python",
				"support.type.primitive",
				"support.type.builtin",
				"variable.language",  //  Exclude `variable.language.generic-parameter`
				"constant.language",
				"storage.type",
				"storage.modifier",
				"support.function.selector-reference",  //  Swift #selector
				"markup.heading",  // Markdown
			],
			"settings": {
				"foreground": "#BB3355",
				"fontStyle": "bold",
			}
		},
		{
			"name": "Plain",
			"scope": [
				"keyword.operator",
				"storage.type.function.arrow",
				"variable.other",
				"meta.embedded",
			],
			"settings": {
				"foreground": "#112244",
				"fontStyle": "",
			}
		},
		{
			"name": "Project Type",
			"scope": [
				"entity.name.type",
				"entity.name.class",
				"entity.name.tag",  // HTML, CSS
				"entity.other.attribute-name",  // CSS
				"variable.language.generic-parameter",
				"variable.language.associatedtype",
			],
			"settings": {
				"foreground": "#0044BB",
				"fontStyle": "",
			}
		},
		{
			"name": "Project Field",
			"scope": [
				"entity.name.function",
				"variable.css",  // CSS
				"variable.less",  // CSS
				"variable.scss",  // CSS
				"variable.sass",  // CSS
				"variable.argument.css",  // CSS
				"variable.argument.less",  // CSS
				"variable.argument.scss",  // CSS
				"variable.argument.sass",  // CSS
				"variable.other.property",
				"variable.other.object.property",
				"meta.object-literal.key",
			],
			"settings": {
				"foreground": "#0077AA",
				"fontStyle": "",
			}
		},
		{
			"name": "Support Type",
			"scope": [
				"support.type",
				"support.class",
				"punctuation.separator.table",  // Markdown
				"punctuation.definition.table",  // Markdown
				"punctuation.definition.quote",  // Markdown
				"punctuation.definition.list.begin.markdown",  // Markdown
				"meta.separator.markdown",  // Markdown
				"entity.other.attribute-name.xml",  //  XML
				"entity.other.attribute-name.html",  //  HTML
			],
			"settings": {
				"foreground": "#7726BB",
				"fontStyle": "",
			}
		},
		{
			"name": "Support Field",
			"scope": [
				"support.function",
				"support.variable",
				"support.constant",
			],
			"settings": {
				"foreground": "#9955AA",
				"fontStyle": "",
			}
		},
		{
			"name": "Italic",
			"scope": [
				"markup.italic",
			],
			"settings": {
				"fontStyle": "italic",
			}
		},
		{
			"name": "Bold",
			"scope": [
				"markup.bold",
			],
			"settings": {
				"fontStyle": "bold",
			}
		},
		{
			"name": "Underline",
			"scope": [
				"markup.underline",
			],
			"settings": {
				"fontStyle": "underline",
			}
		},
		{
			"name": "Strike Through",
			"scope": [
				"markup.strikethrough",
			],
			"settings": {
				"fontStyle": "strikethrough",
			}
		}
	],

