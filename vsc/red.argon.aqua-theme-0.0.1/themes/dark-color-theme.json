{
	"name": "Aqua (Dark)",

	"semanticHighlighting": true,

	"semanticTokenColors": {
		"macro": {
			"foreground": "#CC9944",
			"fontStyle": "",
		},

		//  Local Variables:
		// "parameter": "#E8ECF0",
		"variable": "#E8ECF0",

		//  Project Fields:
		"function": "#88CCFF",
		"property": "#88CCFF",
		"method": "#88CCFF",
		"enumMember": "#88CCFF",

		//  Project Types:
		"type": "#AAEEFF",
		"enum": "#AAEEFF",
		"class": "#AAEEFF",
		"struct": "#AAEEFF",
		"interface": "#AAEEFF",
		"namespace": "#AAEEFF",
		"typeParameter": "#AAEEFF",

		//  Support Fields:
		"variable.builtin": "#CC88FF",
		"function.builtin": "#CC88FF",
		"property.builtin": "#CC88FF",
		"method.builtin": "#CC88FF",
		"enumMember.builtin": "#CC88FF",
		"typeParameter.builtin": "#EEBBFF",
		"variable.defaultLibrary": "#CC88FF",
		"function.defaultLibrary": "#CC88FF",
		"property.defaultLibrary": "#CC88FF",
		"method.defaultLibrary": "#CC88FF",
		"enumMember.defaultLibrary": "#CC88FF",
		"typeParameter.defaultLibrary": "#EEBBFF",
		"magicFunction": "#CC88FF",

		//  Support Types:
		"type.builtin": "#EEBBFF",
		"enum.builtin": "#EEBBFF",
		"class.builtin": "#EEBBFF",
		"struct.builtin": "#EEBBFF",
		"interface.builtin": "#EEBBFF",
		"type.defaultLibrary": "#EEBBFF",
		"enum.defaultLibrary": "#EEBBFF",
		"class.defaultLibrary": "#EEBBFF",
		"struct.defaultLibrary": "#EEBBFF",
		"interface.defaultLibrary": "#EEBBFF",
		"namespace.defaultLibrary": "#EEBBFF",

		// "selfParameter": {
		// 	"foreground": "#FF7788",
		// 	"fontStyle": "bold",
		// }
	},

	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
			],
			"settings": {
				"foreground": "#778899",
				"fontStyle": "",
			}
		},
		{
			"name": "Preprocessor",
			"scope": [
				"meta.preprocessor",
				"markup.inline.raw",  // Markdown
				"markup.fenced_code.block",  // Markdown
			],
			"settings": {
				"foreground": "#CC9944",
				"fontStyle": "",
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"constant.other.color",
				"storage.type.string",
				"string.other.link",  // Markdown
				"constant.other.reference.link",  // Markdown
			],
			"settings": {
				"foreground": "#99BB00",
				"fontStyle": "",
			}
		},
		{
			"name": "Number",
			"scope": [
				"constant.numeric",
				"string.quoted.single.c",
				"string.quoted.single.cpp",
				"string.quoted.single.objc",
				"string.quoted.single.objcpp",
				"storage.type.numeric",
				"storage.type.number",
				"keyword.other.unit",  // CSS
				"entity.other.keyframe-offset.percentage",  // CSS
			],
			"settings": {
				"foreground": "#EEBB00",
				"fontStyle": "",
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword.control",
				"keyword.other",
				"keyword.operator.new",
				"keyword.operator.delete",
				"keyword.operator.sizeof",
				"keyword.operator.alignof",
				"keyword.operator.typeid",
				"keyword.operator.expression",
				"keyword.operator.logical.python",
				"support.type.primitive",
				"support.type.builtin",
				"variable.language",  //  Exclude `variable.language.generic-parameter`
				"constant.language",
				"storage.type",
				"storage.modifier",
				"support.function.selector-reference",  //  Swift #selector
				"markup.heading",  // Markdown
			],
			"settings": {
				"foreground": "#FF7788",
				"fontStyle": "bold",
			}
		},
		{
			"name": "Plain",
			"scope": [
				"keyword.operator",
				"storage.type.function.arrow",
				"variable.other",
				"meta.embedded",
				"punctuation.separator",
			],
			"settings": {
				"foreground": "#E8ECF0",
				"fontStyle": "",
			}
		},
		{
			"name": "Project Type",
			"scope": [
				"entity.name.type",
				"entity.name.class",
				"entity.name.tag",  // HTML, CSS
				"entity.other.attribute-name",  // CSS
				"variable.language.generic-parameter",
				"variable.language.associatedtype",
			],
			"settings": {
				"foreground": "#AAEEFF",
				"fontStyle": "",
			}
		},
		{
			"name": "Project Field",
			"scope": [
				"entity.name.function",
				"variable.css",  // CSS
				"variable.less",  // CSS
				"variable.scss",  // CSS
				"variable.sass",  // CSS
				"variable.argument.css",  // CSS
				"variable.argument.less",  // CSS
				"variable.argument.scss",  // CSS
				"variable.argument.sass",  // CSS
				"variable.other.property",
				"variable.other.object.property",
				"meta.object-literal.key",
			],
			"settings": {
				"foreground": "#88CCFF",
				"fontStyle": "",
			}
		},
		{
			"name": "Support Type",
			"scope": [
				"support.type",
				"support.class",
				"punctuation.separator.table",  // Markdown
				"punctuation.definition.table",  // Markdown
				"punctuation.definition.quote",  // Markdown
				"punctuation.definition.list.begin.markdown",  // Markdown
				"meta.separator.markdown",  // Markdown
				"entity.other.attribute-name.xml",  //  XML
				"entity.other.attribute-name.html",  //  HTML
			],
			"settings": {
				"foreground": "#EEBBFF",
				"fontStyle": "",
			}
		},
		{
			"name": "Support Field",
			"scope": [
				"support.function",
				"support.variable",
				"support.constant",
			],
			"settings": {
				"foreground": "#CC88FF",
				"fontStyle": "",
			}
		},
		{
			"name": "Italic",
			"scope": [
				"markup.italic",
			],
			"settings": {
				"fontStyle": "italic",
			}
		},
		{
			"name": "Bold",
			"scope": [
				"markup.bold",
			],
			"settings": {
				"fontStyle": "bold",
			}
		},
		{
			"name": "Underline",
			"scope": [
				"markup.underline",
			],
			"settings": {
				"fontStyle": "underline",
			}
		},
		{
			"name": "Strike Through",
			"scope": [
				"markup.strikethrough",
			],
			"settings": {
				"fontStyle": "strikethrough",
			}
		}
	],

